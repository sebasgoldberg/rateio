{
    "welcomeFile": "/fiori.html",
    "routes": [ 
        {
            "source": "^/rateio_api/(.*)",
            "target": "$1",
            "destination": "rateio_api",
            "scope": "$XSAPPNAME.rateioAdmin"
        },
        {
            "source": "^/(.*)",
            "localDir": "./",
            "authenticationType": "xsuaa",
            "scope": "$XSAPPNAME.rateioAdmin"
        }
    ]
}