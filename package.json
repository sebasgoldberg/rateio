{
    "name": "rateio",
    "version": "1.0.0",
    "description": "Rateio de documentos.",
    "repository": "https://github.com/sebasgoldberg/rateio.git",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^3",
        "@sap/hana-client": "^2.4.196",
        "@sap/xsenv": "^3.0.0",
        "@sap/xssec": "^2.2.5",
        "binary-search": "^1.3.6",
        "express": "^4.17.1",
        "passport": "^0.4.1",
        "soap": "^0.31.0"
    },
    "scripts": {
        "start": "npx cds run",
        "test": "jest"
    },
    "cds": {
        "requires": {
            "API_JOURNALENTRYITEMBASIC_SRV": {
                "kind": "odata",
                "model": "srv/external/API_JOURNALENTRYITEMBASIC_SRV",
                "credentials": {
                    "destination": "s4hc"
                }
            },
            "db": {
                "kind": "sql",
                "[production]": {
                    "kind": "hana"
                }
            },
            "uaa": {
                "kind": "xsuaa"
            }
        },
        "auth": {
            "passport": {
                "[production]":{
                    "strategy": "JWT"
                },
                "strategy": "mock",
                "users": {
                    "admin": {
                        "roles": [
                            "rateioAdmin"
                        ]
                    }
                }
            }
        }
    },
    "devDependencies": {
        "jest": "^26.0.1",
        "request": "^2.88.2",
        "request-promise": "^4.2.5",
        "sqlite3": "^4.2.0",
        "supertest": "^4.0.2"
    }
}
